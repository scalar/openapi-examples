#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/tsc-alias@1.8.8/node_modules/tsc-alias/dist/bin/node_modules:/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/tsc-alias@1.8.8/node_modules/tsc-alias/dist/node_modules:/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/tsc-alias@1.8.8/node_modules/tsc-alias/node_modules:/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/tsc-alias@1.8.8/node_modules:/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/tsc-alias@1.8.8/node_modules/tsc-alias/dist/bin/node_modules:/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/tsc-alias@1.8.8/node_modules/tsc-alias/dist/node_modules:/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/tsc-alias@1.8.8/node_modules/tsc-alias/node_modules:/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/tsc-alias@1.8.8/node_modules:/Users/mastro/scalar/openapi-examples/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/index.js" "$@"
else
  exec node  "$basedir/../../dist/bin/index.js" "$@"
fi
